{
  "ReverseProxy": {
    "Routes": {
      "user-route": {
        "ClusterId": "user-cluster",
        "Match": {
          "Path": "/user/{**catchall}"
        }
      }
    },
    "Clusters": {
      "user-cluster": {
        "Destinations": {
          "user-destination": {
            "Address": "https://localhost:7242/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}